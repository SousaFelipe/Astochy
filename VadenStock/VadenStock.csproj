    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Icons\clear-search-active.png" />
    <None Remove="Resources\Icons\clear-search.png" />
    <None Remove="Resources\Icons\close-circle.png" />
    <None Remove="Resources\Icons\close.png" />
